{
  "name": "temp_cook_eat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit",
    "pre-push": "npm run type-check && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "node scripts/setup-test-results-dir.js && playwright test",
    "test:e2e:ui": "node scripts/setup-test-results-dir.js && playwright test --ui",
    "test:e2e:debug": "node scripts/setup-test-results-dir.js && playwright test --debug",
    "test:e2e:chrome": "node scripts/setup-test-results-dir.js && playwright test --project=chromium",
    "test:e2e:firefox": "node scripts/setup-test-results-dir.js && playwright test --project=firefox",
    "test:e2e:safari": "node scripts/setup-test-results-dir.js && playwright test --project=webkit",
    "test:e2e:mobile": "node scripts/setup-test-results-dir.js && playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:setup-db": "ts-node e2e/setup/test-database.ts",
    "test:organize-screenshots": "node scripts/organize-screenshots.js",
    "test:report": "node scripts/analyze-test-results.js",
    "test:e2e:with-report": "npm run test:e2e && node scripts/analyze-test-results.js",
    "test:view-latest": "open test-results/latest/reports/index.html || xdg-open test-results/latest/reports/index.html || start test-results/latest/reports/index.html",
    "test:list-results": "ls -la test-results || dir test-results",
    "setup-test-db": "node scripts/setup-test-db.js",
    "setup-preview-db": "node scripts/setup-preview-db.js",
    "test:all": "npm run test && npm run test:e2e",
    "test:preview": "node scripts/setup-test-results-dir.js && TEST_BASE_URL=https://cook-eat-preview.vercel.app playwright test",
    "test:preview:ui": "node scripts/setup-test-results-dir.js && TEST_BASE_URL=https://cook-eat-preview.vercel.app playwright test --ui",
    "test:preview:chrome": "node scripts/setup-test-results-dir.js && TEST_BASE_URL=https://cook-eat-preview.vercel.app playwright test --project=chromium",
    "test:preview:setup": "node scripts/setup-preview-db.js && npm run test:preview",
    "test:preview:view-latest": "open test-results/latest/reports/index.html || xdg-open test-results/latest/reports/index.html || start test-results/latest/reports/index.html",
    "test:preview:with-report": "npm run test:preview && node scripts/analyze-test-results.js",
    "deploy": "vercel --prod",
    "pre-deploy": "node scripts/pre-deploy.js",
    "troubleshoot": "node scripts/troubleshoot-vercel.js",
    "baseline": "node scripts/baseline-migration.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@heroicons/react": "^2.2.0",
    "@mantine/core": "^7.17.0",
    "@mantine/dates": "^7.17.0",
    "@mantine/hooks": "^7.17.0",
    "@prisma/client": "5.10.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tiptap/extension-image": "^2.11.5",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@vercel/blob": "^0.22.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.475.0",
    "next": "14.1.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.5.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.0.2",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@playwright/test": "^1.51.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.25",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "prisma": "5.10.2",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2"
  }
}
